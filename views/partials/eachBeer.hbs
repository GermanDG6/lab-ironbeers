
<div class="col-md-4">
  <div class="card">
    {{#if image_url}}
    <img
      class="card-img-top"
      src={{image_url}}
      alt={{name}}
    />
    {{/if}}
    <div class="card-body">
      <h5 class="card-title">{{name}}/{{tagline}}</h5>
      <p class="card-text">{{description}}</p>
    </div>
  </div>
</div>